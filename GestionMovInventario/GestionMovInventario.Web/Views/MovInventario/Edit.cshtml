@model GestionMovInventario.Models.MovInventario
@{
    ViewBag.Title = "Editar Movimiento";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>Editar Movimiento</h2>
@if (ViewBag.Error != null)
{
    <div class="alert alert-danger">@ViewBag.Error</div>
}
@using (Html.BeginForm("Edit", "MovInventario", FormMethod.Post))
{
    @Html.AntiForgeryToken()
    @Html.HiddenFor(model => model.Cod_CIA)
    @Html.HiddenFor(model => model.Compania_Venta_3)
    @Html.HiddenFor(model => model.Almacen_Venta)
    @Html.HiddenFor(model => model.Tipo_Movimiento)
    @Html.HiddenFor(model => model.Tipo_Documento)
    @Html.HiddenFor(model => model.Nro_Documento)
    @Html.HiddenFor(model => model.Cod_Item_2)

    <div class="form-horizontal">
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })

        @Html.EditorFor(model => model.Proveedor)
        @Html.EditorFor(model => model.Almacen_Destino)
        @Html.EditorFor(model => model.Cantidad)
        @Html.EditorFor(model => model.Doc_Ref_1)
        @Html.EditorFor(model => model.Doc_Ref_2)
        @Html.EditorFor(model => model.Doc_Ref_3)
        @Html.EditorFor(model => model.Doc_Ref_4)
        @Html.EditorFor(model => model.Doc_Ref_5)

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Guardar" class="btn btn-success" />
            </div>
        </div>
    </div>
}
<a href="@Url.Action("Index")">Volver a la lista</a>
