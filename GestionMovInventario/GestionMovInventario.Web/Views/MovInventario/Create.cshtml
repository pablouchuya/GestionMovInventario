@model GestionMovInventario.Models.MovInventario
@{
    ViewBag.Title = "Crear Movimiento";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>Crear Movimiento</h2>
@if (ViewBag.Error != null)
{
    <div class="alert alert-danger">@ViewBag.Error</div>
}
@using (Html.BeginForm("Create", "MovInventario", FormMethod.Post))
{
    @Html.AntiForgeryToken()
<div class="form-horizontal">
    <hr />
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })

    @Html.EditorFor(model => model.Cod_CIA)
    @Html.EditorFor(model => model.Compania_Venta_3)
    @Html.EditorFor(model => model.Almacen_Venta)
    @Html.EditorFor(model => model.Tipo_Movimiento)
    @Html.EditorFor(model => model.Tipo_Documento)
    @Html.EditorFor(model => model.Nro_Documento)
    @Html.EditorFor(model => model.Cod_Item_2)
    @Html.EditorFor(model => model.Proveedor)
    @Html.EditorFor(model => model.Almacen_Destino)
    @Html.EditorFor(model => model.Cantidad)
    @Html.EditorFor(model => model.Doc_Ref_1)
    @Html.EditorFor(model => model.Doc_Ref_2)
    @Html.EditorFor(model => model.Doc_Ref_3)
    @Html.EditorFor(model => model.Doc_Ref_4)
    @Html.EditorFor(model => model.Doc_Ref_5)

    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" value="Crear" class="btn btn-primary" />
        </div>
    </div>
</div>
}
<a href="@Url.Action("Index")">Volver a la lista</a>